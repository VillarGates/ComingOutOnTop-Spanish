translate languagetwo alex_appears_8681db94:

    # a "I'm a Scotch man myself. Lagavulin's my favorite."
    a "Soy escocés. Lagavulin es mi favorito."

# game/script_alex.rpy:203
translate languagetwo alex_appears_fdfcabd1:

